@article{ophir_news_2021,
	title = {News media framing of social protests around racial tensions during the {Donald} {Trump} presidency - {Yotam} {Ophir}, {Devin} {K} {Forde}, {Madison} {Neurohr}, {Dror} {Walter}, {Virginia} {Massignan}, 2021},
