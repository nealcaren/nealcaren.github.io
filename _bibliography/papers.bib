}

@article{majed_defense_2021,
	title = {In {Defense} of {Intra}-{Sectarian} {Divide}: {Street} {Mobilization}, {Coalition} {Formation}, and {Rapid} {Realignments} of {Sectarian} {Boundaries} in {Lebanon}},
