}

@article{vliegenthart_media_2016,
	title = {The {Media} as a {Dual} {Mediator} of the {Political} {Agenda}–{Setting} {Effect} of {Protest}. {A} {Longitudinal} {Study} in {Six} {Western} {European} {Countries}},
