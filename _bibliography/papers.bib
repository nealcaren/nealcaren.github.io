	pages = {105--128},
	file = {Coppock et al. - 2016 - When Treatments are Tweets A Network Mobilization.pdf:/Users/nealcaren/Zotero/storage/D3ND3RR7/Coppock et al. - 2016 - When Treatments are Tweets A Network Mobilization.pdf:application/pdf;Snapshot:/Users/nealcaren/Zotero/storage/LE7D424Z/s11109-015-9308-6.html:text/html},
